using namespace std;
#include <iostream>
#include <iomanip>
#include <string>
#include <ncurses.h>
#include <vector>

int main () {
   vector <int> buffer;
   for (int i = 0; i < 10; i++)
	buffer.push_back(i);
void print(){
   cout << "buffer contains: ";
   for (int i = 0; i < buffer.size(); i++)
	cout << ' ' << buffer[i];
   cout << endl;
}
   buffer.erase(4, buffer.size());
   print();

   return 0;
}

